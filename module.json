			"menu" : "Track",
			"callback" : "master_volume",
			"parameters" : {
					
				"Fader Level": {
					"type": "Float",
					"default": 0.75, 
					"min":0,
					"max":1,
					"useForMapping": true	}  } 
			},
		
		"Track Volume" : {
			"menu" : "Track",
			"callback" : "volume",
			"parameters" : {
						
