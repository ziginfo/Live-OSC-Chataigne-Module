{
	"name": "Live OSC Advanced",
	"type": "OSC",
	"path": "Software",
	
	"version": "1.0",
	"description": "Controls Live with Chataigne via OSC",
	"url":"https://github.com/ziginfo/Live-OSC-Chataigne-Module",
	"downloadURL": "https://github.com/ziginfo/Live-OSC-Chataigne-Module/archive/refs/heads/main.zip",
	
	"hasInput": true,
	"hasOutput": true, 

	"hideDefaultCommands": true,
	"hideDefaultParameters": [
		"colorSendMode",
		"autoRange",
		"autoFeedback",
		"pass_through",
		"input",
		"outputs",
		"useHierarchy",
		"colorSendMode",
		"booleanSendMode"
		],
	
	"defaults": {
	
		"OSC Input": {
			"localPort" : 11001 },
			
		"OSC Outputs": {
			"OSC Output": {
				"local" : false,
				"remotePort" : 11000,
				"listenToFeedback":false } }
	},
	
	"parameters": { 
	
	"Advice":
		{ "type": "String",
			"description":"SaveReload the Session after changing this value !",
			"default":"Save and Reload the Session after changing this value !" },
				
	"Number of Tracks":
		{ "type": "Integer",
			"description":"Reload the Project after changing this value !",
			"min": 8 },
					
	"Auto Add":{"type":"Boolean"}
	},
	
	"scripts": ["live.js"],
	
	"values":
  		{  },
	
	"commands": {
	
