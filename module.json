{
	"name": "Live OSC Advanced",
	"type": "OSC",
	"path": "Software",
	
	"version": "1.2",
	"description": "Controls Live with Chataigne via OSC",
	"url":"https://github.com/ziginfo/Live-OSC-Chataigne-Module",
	"downloadURL": "https://github.com/ziginfo/Live-OSC-Chataigne-Module/archive/refs/heads/main.zip",
	
	"hasInput": true,
	"hasOutput": true, 

	"hideDefaultCommands": false,
	"hideDefaultParameters": [
		"colorSendMode",
		"autoRange",
		"autoFeedback",
		"pass_through",
		"autoAdd",
		"input",
		"outputs",
		"useHierarchy",
		"colorSendMode",
		"booleanSendMode"
		],
	
	"defaults": {
	
		"autoAdd":false,
		
		"OSC Input": {
			"localPort" : 11001 },
			
		"OSC Outputs": {
			"OSC Output": {
				"local" : false,
				"remotePort" : 11000,
				"listenToFeedback":false } }
	},
	
	"parameters": { 
	
				 
		"Advice":
			{ "type": "String",
			"description":"Reload the Session after changing this value !",
			"default":"Reload the Session after changing these values !" },
				
		"Number of Tracks":
			{ "type": "Integer",
			"description":"Reload the Project after changing this value !",
			"min": 8 },
			
		"Number of Scenes":
			{ "type": "Integer",
			"description":"Reload the Project after changing this value !",
			"min": 4 }
	},
	
	"scripts": ["live.js"],
	
	"values":  { 
	
		"Tracks":
		{ "type":"Container",
			"collapsed":true},
			 
		"Clips":
		{ "type":"Container",
			"collapsed":true}
	 },
	
	"commands": {
	
	"Master Volume" : {
			"menu" : "Track",
			"callback" : "master_volume",
			"parameters" : {
					
				"Fader Level": {
					"type": "Float",
					"default": 0.75, 
					"min":0,
					"max":1,
					"useForMapping": true	}  } 
			},
		
		"Track Volume" : {
			"menu" : "Track",
			"callback" : "volume",
			"parameters" : {
						
				"Track No": {
					"type": "Integer",
					"default": 1, 
					"min":1,			
					"useForMapping": true	}, 
					
				"Fader Level": {
					"type": "Float",
					"default": 0.75, 
					"min":0,
					"max":1,
					"useForMapping": true	}  } 
			},
			
		"Track Pan" : {
			"menu" : "Track",
			"callback" : "pan",
			"parameters" : {
						
				"Track No": {
					"type": "Integer",
					"default": 1, 
					"min":1,
					"useForMapping": true	}, 
					
				"Pan": {
					"type": "Float",
					"default": 0.5, 
					"min":-1,
					"max":1,
					"useForMapping": true	}  } 
			},
		
		"Track Mute" : {
			"menu" : "Track",
			"callback" : "mute",
			"parameters" : {
						
				"Track No": {
					"type": "Integer",
					"default": 1, 
					"min":1,
					"useForMapping": true	},
					
				"Mute on/off": {
					"type": "Boolean",
					"default": 1, 
					"min":0,
					"max":1,
					"useForMapping": true }  } 
			},
			
		"Track Solo" : {
			"menu" : "Track",
			"callback" : "solo",
			"parameters" : {
						
				"Track No": {
					"type": "Integer",
					"default": 1, 
					"min":1,
					"useForMapping": true	},
					
					"Solo on/off": {
					"type": "Boolean",
					"default": 1, 
					"min":0,
					"max":1,
					"useForMapping": true }  } 
			},
			
		"Track Select" : {
			"menu" : "Track",
			"callback" : "select",
			"parameters" : {
						
				"Track No": {
					"type": "Integer",
					"default": 1, 
					"min":1,
					"useForMapping": true	}	} 
			},
			
		"Track Send" : {
			"menu" : "Track",
			"callback" : "send",
			"parameters" : {
						
				"Track No": {
					"type": "Integer",
					"default": 1, 
					"min":1,									
					"useForMapping": true	},
					
				"Send No": {
					"type": "Integer",
					"default": 1, 
					"min":1,				
					"useForMapping": true	},  
					
				"Send Level": {
					"type": "Float",
					"default": 0.75, 
					"min":0,
					"max":1,
					"useForMapping": true	} } 
			},
			
		"Fire Clip" : {
			"menu" : "Clips",
			"callback" : "fire_clip",
			"parameters" : {
						
				"Track ID": {
					"type": "Integer",
					"default": 1, 
					"min":1,				
					"useForMapping": true	},
					
				"Clip ID": {
					"type": "Integer",
					"default": 1, 
					"min":1,				
					"useForMapping": true	} }
			},
			
		"Stop Clip" : {
			"menu" : "Clips",
			"callback" : "stop_clip",
			"parameters" : {
						
				"Track ID": {
					"type": "Integer",
					"default": 1, 
					"min":1,				
					"useForMapping": true	},
					
				"Clip ID": {
					"type": "Integer",
					"default": 1, 
					"min":1,				
					"useForMapping": true	} }
			},
			
		"Stop Track Clips" : {
			"menu" : "Clips",
			"callback" : "stop_track_clips",
			"parameters" : {
						
				"Track ID": {
					"type": "Integer",
					"default": 1, 
					"min":1,				
					"useForMapping": true	} }
			},
			
			"Stop All Clips" : {
			"menu" : "Clips",
			"callback" : "stop_all_clips"
			},
				
		"Play Scene" : {
			"menu" : "Scene",
			"callback" : "play_scene",
			"parameters" : {
						
				"Scene No": {
					"type": "Integer",
					"default": 1, 
					"min":1,				
					"useForMapping": true	} }
			},
			
		"Play" : {
			"menu" : "PlayActions",
			"callback" : "play"
			},
			
		"Stop" : {
			"menu" : "PlayActions",
			"callback" : "stop"
			},
			
		"Cycle" : {
			"menu" : "PlayActions",
			"callback" : "cycle",
			"parameters" : {					
				"On/Off": {
					"type": "Boolean",
					"default": 1, 
					"min":0,
					"max":1,
					"useForMapping": true }  }
			},
			
		"Rec" : {
			"menu" : "PlayActions",
			"callback" : "rec"
			},
			
		"Click" : {
			"menu" : "PlayActions",
			"callback" : "click"
			},
			
			
		"Rewind" : {
			"menu" : "PlayActions",
			"callback" : "rewind"
			},
			
		"Tap Tempo" : {
			"menu" : "PlayActions",
			"callback" : "taptempo"
			},
			
		"New Audio Track" : {
			"menu" : "Song Actions",
			"callback" : "audio_track",
			"parameters" : {		
				"New Track ID": {
					"type": "Integer",
					"default": 1, 
					"min":0,				
					"useForMapping": true	} }
			},
			
		"New Midi Track" : {
			"menu" : "Song Actions",
			"callback" : "midi_track",
			"parameters" : {		
				"New Track ID": {
					"type": "Integer",
					"default": 1, 
					"min":0,				
					"useForMapping": true	} }
			},
			
		"New Aux Track" : {
			"menu" : "Song Actions",
			"callback" : "aux_track"
			},
			
		"Add New Scene" : {
			"menu" : "Scene",
			"callback" : "new_scene",
			"parameters" : {
				"Advice :": {
					"type": "String",
					"default": "0 : adds scene at the end!" },		
				"Insert New Scene ID": {
					"type": "Integer",
					"default": 0, 
					"min": 0,				
					"useForMapping": true	} }
				},
				
		"Duplicate Scene" : {
			"menu" : "Scene",
			"callback" : "duplicate_scene",
			"parameters" : {		
				"Scene ID": {
					"type": "Integer",
					"default": 1, 
					"min":0,				
					"useForMapping": true	} }
				},
			
		"Delete Scene" : {
			"menu" : "Scene",
			"callback" : "delete_scene",
			"parameters" : {		
				"Scene ID": {
					"type": "Integer",
					"default": 1, 
					"min":0,				
					"useForMapping": true	} }
				},
				
		"Set Tempo" : {
			"menu" : "Song Actions",
			"callback" : "set_tempo",
			"parameters" : {		
				"New Track ID": {
					"type": "Integer",
					"default": 120, 
					"min":30,				
					"useForMapping": true	} }
			},
	
		"Prev Marker" : {
			"menu" : "Markers",
			"callback" : "prev_mark"
			},
			
		"Next Marker" : {
			"menu" : "Markers",
			"callback" : "next_mark"
			},
			
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	

		
	

		
	
		
	
		
	}
}
